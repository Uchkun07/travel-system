{
  "groups": [
    {
      "name": "jwt",
      "type": "org.example.springproject.config.JwtProperties",
      "sourceType": "org.example.springproject.config.JwtProperties"
    }
  ],
  "properties": [
    {
      "name": "jwt.secret",
      "type": "java.lang.String",
      "description": "JWT签名密钥",
      "sourceType": "org.example.springproject.config.JwtProperties",
      "defaultValue": "travel-system-secret-key-for-jwt-token-generation-and-validation-2024"
    },
    {
      "name": "jwt.expiration",
      "type": "java.lang.Long",
      "description": "JWT令牌过期时间(毫秒),用于记住我功能",
      "sourceType": "org.example.springproject.config.JwtProperties",
      "defaultValue": 604800000
    },
    {
      "name": "jwt.expiration-short",
      "type": "java.lang.Long",
      "description": "JWT令牌短期过期时间(毫秒),用于普通登录",
      "sourceType": "org.example.springproject.config.JwtProperties",
      "defaultValue": 86400000
    }
  ]
}
